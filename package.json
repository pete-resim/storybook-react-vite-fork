{
  "name": "@pete-resim/react-vite",
  "version": "0.0.1",
  "description": "Storybook for React and Vite: Develop, document, and test UI components in isolation",
  "keywords": [
    "storybook",
    "storybook-framework",
    "react",
    "vite",
    "component",
    "components"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/next/code/frameworks/react-vite",
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/storybook-react-vite-fork.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./package.json": "./package.json",
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    }
  },
  "files": [
    "dist/**/*",
    "template/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@pete-resim/vite-plugin-react-docgen-typescript": "0.0.3",
    "@rollup/pluginutils": "^5.0.2",
    "@storybook/builder-vite": "10.0.8",
    "@storybook/react": "10.0.8",
    "empathic": "^2.0.0",
    "magic-string": "^0.30.0",
    "react-docgen": "^8.0.0",
    "resolve": "^1.22.8",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "storybook": "^10.0.8",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
